{{extend 'layout.html'}}
	
	 <nav class="navbar navbar-expand-md navbar-dark bg-dark">
	    <div class="container"> <button class="navbar-toggler navbar-toggler-right border-0" type="button" data-toggle="collapse" data-target="#navbar10">
	        <span class="navbar-toggler-icon"></span>
	      </button>
	      <div class="collapse navbar-collapse" id="navbar10">
	        <ul class="navbar-nav ml-auto">
	          <li class="nav-item"> <a class="nav-link" href="{{=URL('projeto','acesso_geral_projeto', args=projeto.id)}}">Acesso Geral</a> </li>
	          <li class="nav-item"> <a class="nav-link" href="#">Prestação Venda</a> </li>
	          <li class="nav-item"> <a class="nav-link" href="#">Investimento</a> </li>
	          <li class="nav-item"> <a class="nav-link" href="#">Prestação Cobrança</a> </li>
	          <li class="nav-item"> <a class="nav-link" href="#">Relatorio Lucros</a> </li>
	        </ul>
	      </div>
	    </div>
	  </nav>
	<div class="section">
      <div class="container">
        <div class="row">
          <div class="col-md-12">
	<h2>Lista de Envio {{=projeto.nome_chefe}}</h2>
			
	
	          <table class="table">
	            <thead>
	              <tr>
	                <th>Nome</th>
	                <th>Fichas</th>
	                  <th>Entrada</th>
	                  <th>Venda</th>
	                <th>Comissao</th> 
	                  <th>Vale</th>
	                <th>Caderno</th> 
	                  <th>Saldo</th>
	                <th>Recebido</th>
	              </tr>
	            </thead>
	            <tbody>
	
	                {{totalfichas=0.0}}
	                {{totalentrada=0.0}}
	                {{totalvenda=0.0}}
	                {{totalcomissao=0.0}}
	                {{totalcaderno=0.0}}
	                {{totalrecebido=0.0}}
	                {{totalvale=0.0}}
	
					{{for row in rows:}}
	              <tr> 
	                  <th><a href="{{=URL('alterar_vendedor_venda', args=row.id)}}">{{=row.nome_vendedor}}</a></th>
	                <th>{{=row.quant_fichas}}</th>
	                  <th>{{=row.entradas_venda}}</th>
	                  <th>{{=row.venda}}</th>
	                  {{comiss=row.venda*row.comissao_venda/100}}
	                <th>{{=comiss}}</th> 
	                  <th>{{=row.vale_saida}}</th>
	                <th>{{=row.vale_caderno}}</th> 
	                  <th>{{=comiss-(row.vale_saida)-(row.vale_caderno)}}</th>
	
	                  {{totalfichas=totalfichas+row.quant_fichas}}
	                   {{totalentrada=totalentrada+row.entradas_venda}}
	                {{totalvenda=totalvenda+row.venda}}
	                  {{totalcomissao=totalcomissao+comiss}}
	                  {{totalvale=totalvale+row.vale_saida}}
	                {{totalcaderno=totalcaderno+row.vale_caderno}}
	              </tr>
	              {{pass}}
					 <tr>
	                 <th>Total</th>
	                 <th>{{=totalfichas}}</th>
	                 <th>{{=totalentrada}}</th>
	                 <th>{{=totalvenda}}</th>
	                 <th>{{=totalcomissao}}</th>
	                 <th>{{=totalvale}}</th>
	                 <th>{{=totalcaderno}}</th>
	                 <th>--></th>
	                 <th>{{=totalrecebido}}</th>
	                     
	              </tr>
	            </tbody>
	          </table>
	{{projeto.total_vale_caderno_vendedor=totalcaderno}}
	{{projeto.total_vale_saida_vendedor=totalvale}}
	{{projeto.total_venda=totalvenda}}
	{{projeto.total_entradas_venda=totalentrada}}
	{{projeto.total_quant_fichas=totalfichas}}
	{{projeto.total_comis_venda_vendedor=totalcomissao}}
	{{projeto.total_recebido_chegada_venda_vendedor=totalrecebido}}
	                        {{projeto.update_record()}}
